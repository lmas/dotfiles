
Host github.com
    PubkeyAuthentication yes
    Hostname github.com
    Port 22
    User git
    IdentityFile ~/.ssh/github

Host git
    PubkeyAuthentication yes
    Hostname localhost
    Port 8022
    User git
    IdentityFile ~/.ssh/git

Host homebox
    PubkeyAuthentication yes
    Hostname 192.168.1.2
    Port 8822
    User lmas
    IdentityFile ~/.ssh/homebox

Host pibox
    PubkeyAuthentication yes
    Hostname 192.168.1.3
    Port 8822
    User lmas
    IdentityFile ~/.ssh/pibox

Host tvbox
        PubKeyAuthentication yes
        Hostname 192.168.1.204
        Port 8822
        User lmas
        IdentityFile ~/.ssh/tvbox

Host *
    PubkeyAuthentication no
    IdentitiesOnly yes

